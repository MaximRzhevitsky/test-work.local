{{! Garvity forms Section Break Field component https://docs.gravityforms.com/section-break-css/ }}

<div class="gfield gfield_section_break gsection {{gformAddCustomClass
	customClass}} field_sublabel_below field_description_below gfield_visibility_visible">
	<div class="gsection_title">{{labelText}}</div>
	{{#if descriptionText}}
		<div class="gsection_description">{{descriptionText}}</div>
	{{/if}}
</div>
